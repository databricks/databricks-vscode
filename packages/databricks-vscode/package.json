{
    "name": "databricks",
    "displayName": "Databricks for Visual Studio Code",
    "description": "IDE support for Databricks",
    "publisher": "databricks",
    "license": "Apache-2.0",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Data Science",
        "Machine Learning"
    ],
    "keywords": [
        "spark",
        "databricks",
        "machine learning",
        "ml",
        "data science",
        "ds",
        "lakehouse"
    ],
    "preview": true,
    "badges": [],
    "icon": "images/databricks-logo.png",
    "galleryBanner": {
        "theme": "light"
    },
    "homepage": "https://https://github.com/databricks/databricks-vscode/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/databricks/databricks-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/databricks/databricks-vscode.git"
    },
    "activationEvents": [
        "onCommand:databricks.helloWorld"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "databricks.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "vscode:package": "vsce package --yarn",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "fix": "prettier . --write",
        "test:lint": "eslint src --ext ts && prettier . -c",
        "test:compile": "tsc -p . --outDir out",
        "test:watch": "tsc -p . -w --outDir out",
        "test:run": "node ./out/test/runTest.js",
        "test": "yarn run test:compile && yarn run compile && yarn run test:lint && yarn run test:run",
        "build": "yarn run vscode:prepublish",
        "clean": "rm -rf node_modules out dist .vscode-test"
    },
    "dependencies": {
        "@databricks/databricks-sdk": "*"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "prettier": "^2.7.1",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.4",
        "vsce": "^2.9.1",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
    }
}
