{
    "name": "databricks",
    "displayName": "Databricks for Visual Studio Code",
    "description": "IDE support for Databricks",
    "publisher": "databricks",
    "license": "Apache-2.0",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Data Science",
        "Machine Learning"
    ],
    "keywords": [
        "spark",
        "databricks",
        "machine learning",
        "ml",
        "data science",
        "ds",
        "lakehouse"
    ],
    "preview": true,
    "badges": [],
    "icon": "images/databricks-logo.png",
    "galleryBanner": {
        "theme": "light"
    },
    "homepage": "https://https://github.com/databricks/databricks-vscode/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/databricks/databricks-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/databricks/databricks-vscode.git"
    },
    "activationEvents": [
        "onCommand:databricks.helloWorld"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "databricks.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "vscode:package": "vsce package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "fix": "prettier . --write",
        "lint": "eslint src --ext ts && prettier . -c",
        "test": "node ./out/test/runTest.js",
        "build": "npm run vscode:prepublish"
    },
    "dependencies": {
        "@databricks/databricks-sdk": "*"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.68.0",
        "@vscode/test-electron": "^2.1.3",
        "glob": "^8.0.3",
        "ts-loader": "^9.3.0",
        "vsce": "^2.9.1",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
    }
}
